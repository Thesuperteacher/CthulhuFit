{
  "expo": {
    "name": "CthulhuFit",
    "slug": "cthulhufit",
    "version": "1.0.0",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "web": {
      "bundler": "metro",
      "output": "single"
    },
    "splash": {
      "backgroundColor": "#fcfdf6",
      "image": "./assets/splash.png",
      "resizeMode": "contain"
    },
    "icon": "./assets/icon.png",
    "backgroundColor": "#fcfdf6",
    "primaryColor": "#046F03",
    "plugins": [
      "expo-router",
      [
        "expo-dev-client",
        {
          "launchMode": "most-recent"
        }
      ],
      "expo-localization",
      [
        "react-native-edge-to-edge",
        {
          "android": {
            "parentTheme": "Default",
            "enforceNavigationBarContrast": false
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "scheme": "cthulhufit",
    "android": {
      "package": "com.thesuperteacher.cthulhufit",
      "permissions": [
        "com.android.vending.BILLING"
      ],
      "adaptiveIcon": {
        "backgroundColor": "#dfedda",
        "foregroundImage": "./assets/icon_foreground.png",
        "monochromeImage": "./assets/icon_monochrome.png"
      }
    },
    "ios": {
      "bundleIdentifier": "com.thesuperteacher.cthulhufit",
      "supportsTablet": true,
      "infoPlist": {
        "UIViewControllerBasedStatusBarAppearance": true,
        "ITSAppUsesNonExemptEncryption": false,
        "CFBundleLocalizations": [
          "ar",
          "cs",
          "de",
          "es",
          "fi",
          "fr",
          "it",
          "nl",
          "pt",
          "ru",
          "sr",
          "sv",
          "uk",
          "pl"
        ]
      },
      "appleTeamId": "",
      "version": "$(APP_DISPLAY_VERSION)",
      "buildNumber": "$(APP_VERSION)"
    },
    "notification": {
      "icon": "./assets/notification.png"
    },
    "jsEngine": "hermes"
  }
}
